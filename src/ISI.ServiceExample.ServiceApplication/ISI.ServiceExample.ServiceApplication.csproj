			<CopyToOutputDirectory>Always</CopyToOutputDirectory>
		</None>
		<None Update="Configuration\ConnectionStrings\connectionStrings.config">
			<CopyToOutputDirectory>Always</CopyToOutputDirectory>
		</None>
		<None Update="Configuration\ConnectionStrings\connectionStrings.production.config">
			<CopyToOutputDirectory>Always</CopyToOutputDirectory>
		</None>
